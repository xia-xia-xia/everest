.header {
  height: 350rpx;
  background-color: #00bfff;
  text-align: center;
  border-bottom-left-radius: 10%;
  border-bottom-right-radius: 10%;
}
.userinfo {
  padding-top: 50rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.userinfo-avatar {
  width: 180rpx;
  height: 180rpx;
  margin: 20rpx;
  border-radius: 50%;
  border: 2px solid #fff;
}
.userinfo-nickname {
  color: #fff;
}
.card-add {
  width: 100rpx;
  height: 100rpx;
  border-radius: 8rpx;
  margin-left: 320rpx;
}
.new {
  text-align: center;
}
.workbench {
  border-radius: 5%;
  background: #f2f2f2;
  font-size: 30rpx;
  padding-bottom: 30rpx;
  margin-bottom: 10rpx;
}
.title {
  font-size: 40rpx;
  padding: 20rpx 20rpx;
  margin-bottom: 30rpx;
  display: block;
}
.list {
  display: flex;
  flex-direction: row;
  flex: 1;
}
.items {
  width: 100rpx;
  flex: 1;
  text-align: center;
}
.workbench .items image {
  width: 100rpx;
  height: 100rpx;
}
.workbench .items text {
  display: block;
  text-align: center;
  margin-top: 10rpx;
}



